  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "react-error-overlay": "6.0.9"
  },
  "transform": {
    "\\.(ts|tsx|js|jsx)$": "<rootDir>/node_modules/babel-jest"
  }
