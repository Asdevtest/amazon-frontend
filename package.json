{
  "name": "amazon-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "eject": "react-app-rewired eject",
    "lint": "eslint -c .eslintrc.json \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettify": "prettier --config .prettierrc.json --write \"src/**/*.{js,jsx,ts,tsx}\" --parser typescript",
    "prettify:check": "prettier --config .prettierrc.json --check \"src/**/*.{js,jsx,ts,tsx}\" --parser typescript",
    "fetch-rest-api-schema": "curl https://amazonapi.fvds.ru/documentation/json -o schema.json",
    "generate-rest-api-service": "openapi-generator-cli generate -i ./schema.json -g typescript-axios -c ./src/services/rest-api-service/options-openapi.json -o ./src/services/rest-api-service/codegen/ --skip-validate-spec",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "typecheck": "tsc",
    "translations:filter": "node scripts/filter-unused-translations.js"
  },
