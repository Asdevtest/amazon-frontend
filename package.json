    "@types/react-router-dom": "^5.3.0",
    "axios": "^0.22.0",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.1",
    "clsx": "^1.1.1",
    "compare-versions": "^6.0.0-rc.1",
    "date-fns": "^2.25.0",
    "dayjs": "^1.11.7",
    "draft-js": "^0.11.7",
    "emoji-mart": "^5.2.2",
    "he": "^1.2.0",
