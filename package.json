{
  "name": "amazon-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint -c .eslintrc.json \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettify": "prettier --config .prettierrc.json --write \"src/**/*.{js,jsx}\" --parser typescript",
    "prettify:check": "prettier --config .prettierrc.json --check \"src/**/*.{js,jsx}\" --parser typescript",
    "generate-rest-api-service": "openapi-generator-cli generate -i ./schema.json -g javascript -c ./src/services/rest-api-service/options-openapi.json -o ./src/services/rest-api-service/codegen/ --skip-validate-spec",
    "prepare": "husky install",
    "lint:fix": "eslint --fix --ext .js,.jsx ."
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@date-io/date-fns": "1.x",
    "@material-ui/core": "^4.11.4",
    "@material-ui/data-grid": "^4.0.0-alpha.32",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.11.4",
    "@mui-treasury/styles": "^1.13.1",
    "axios": "^0.21.1",
    "@types/classnames": "^2.3.1",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "^2.3.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "class-transformer": "^0.4.0",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.1",
    "clsx": "^1.1.1",
    "customize-cra": "^1.0.0",
    "date-fns": "^2.22.1",
    "husky": "^6.0.0",
    "js-cookie": "^2.2.1",
    "mobx": "^6.3.2",
    "mobx-persist-store": "^1.0.1",
    "mobx-react": "^7.2.0",
    "mui-treasury": "^0.1.5-alpha.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-material-ui-carousel": "^2.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "superagent": "^6.1.0",
    "typescript": "^4.3.5",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "react-app-rewired": "^2.1.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
