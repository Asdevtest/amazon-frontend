    "ts-jest": "^29.0.5",
    "prettier": "^2.5.0",
    "customize-cra": "^1.0.0",
    "@typescript-eslint/parser": "^5.40.0",
    "husky": "^7.0.2",
    "babel-jest": "^29.3.1",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
