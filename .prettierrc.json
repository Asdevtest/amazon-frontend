{
  "useTabs": false,
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "bracketSpacing": false,
  "arrowParens": "avoid",
  "importOrder": [
    "^react$",
    "^(?:@)?(react-|material)|^(?!react)([a-z]+)",
    "^@constants/(.*)$",
    "@models/authentication-model",
    "./src/models/authentication-model",
    "^@models/(.*)$",
    "^@navigation/(.*)$",
    "^@services/(.*)$",
    "^@styles/(.*)$",
    "^@components/(.*)$",
    "^@views/(.*)$",
    "^@utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "experimentalBabelParserPluginsList": ["typescript", "jsx", "classProperties"]
}
