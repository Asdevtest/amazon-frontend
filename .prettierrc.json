{
  "useTabs": false,
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "semi": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "importOrder": [
    "^react$",
    "^(?:@)?(react-|material)|^(?!react)([a-z]+)",
    "^@constants/(.*)$",
    "^@models(.*)$",
    "^@navigation/(.*)$",
    "^@services/(.*)$",
    "^@styles/(.*)$",
    "^@components/(.*)$",
    "^@views/(.*)$",
    "^@utils/(.*)$",
    "^@contexts/(.*)$",
    "^@typings/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "endOfLine": "auto",
  "experimentalBabelParserPluginsList": ["typescript", "jsx", "classProperties", "decorators-legacy"]
}
